# Copy this file to .env.local and fill in your values

# Supabase Local Development
SUPABASE_URL=http://127.0.0.1:54321
SUPABASE_ANON_KEY=your_local_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_local_service_key_here

# Production Supabase (for deployment)
SUPABASE_PROD_URL=your_production_url_here
SUPABASE_PROD_ANON_KEY=your_production_anon_key_here
SUPABASE_PROD_SERVICE_ROLE_KEY=your_production_service_key_here

# Storage - Backblaze B2
BACKBLAZE_B2_APPLICATION_KEY_ID=your_key_id_here
BACKBLAZE_B2_APPLICATION_KEY=your_key_here
BACKBLAZE_B2_BUCKET_NAME=your_bucket_name_here

# External Services
OPENAI_API_KEY=your_openai_key_here

# Analytics (optional)
ANALYTICS_API_KEY=your_analytics_key_here

# Development
NODE_ENV=development
LOG_LEVEL=debug

# Database (for migrations and local development)
DATABASE_URL=postgresql://postgres:postgres@127.0.0.1:54322/postgres

# JWT Secret (for local development - Supabase will provide this)
JWT_SECRET=your_jwt_secret_here

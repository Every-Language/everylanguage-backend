[
  {
    "corsRuleName": "production-web-upload-access",
    "allowedOrigins": ["https://projects.everylanguage.com"],
    "allowedHeaders": [
      "authorization",
      "content-type",
      "content-length",
      "x-bz-file-name",
      "x-bz-content-sha1"
    ],
    "allowedOperations": [
      "b2_upload_file",
      "b2_download_file_by_id",
      "b2_download_file_by_name"
    ],
    "exposeHeaders": [
      "x-bz-content-sha1",
      "x-bz-file-name",
      "x-bz-file-id",
      "x-bz-upload-timestamp"
    ],
    "maxAgeSeconds": 3600
  },
  {
    "corsRuleName": "downloadFromAnyHttpsOrigin",
    "allowedOrigins": ["https"],
    "allowedHeaders": ["authorization", "range"],
    "allowedOperations": ["b2_download_file_by_id", "b2_download_file_by_name"],
    "exposeHeaders": ["x-bz-content-sha1"],
    "maxAgeSeconds": 3600
  },
  {
    "corsRuleName": "s3DownloadFromAnyHttpsOrigin",
    "allowedOrigins": ["https"],
    "allowedHeaders": ["authorization", "range"],
    "allowedOperations": ["s3_head", "s3_get"],
    "exposeHeaders": ["x-bz-content-sha1"],
    "maxAgeSeconds": 3600
  }
]

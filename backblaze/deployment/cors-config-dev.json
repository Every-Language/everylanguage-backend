{
  "environment": "development",
  "corsRules": [
    {
      "corsRuleName": "localhost-dev-upload-access",
      "allowedOrigins": [
        "http://localhost:5173",
        "http://127.0.0.1:5173",
        "http://localhost:3000",
        "http://127.0.0.1:3000"
      ],
      "allowedHeaders": ["*"],
      "allowedOperations": [
        "b2_upload_file",
        "b2_download_file_by_id",
        "b2_download_file_by_name"
      ],
      "exposeHeaders": [
        "x-bz-content-sha1",
        "x-bz-file-name",
        "x-bz-file-id",
        "x-bz-upload-timestamp",
        "content-length",
        "content-type",
        "content-range",
        "content-disposition"
      ],
      "maxAgeSeconds": 3600
    }
  ]
}

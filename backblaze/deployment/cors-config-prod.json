{
  "environment": "production",
  "corsRules": [
    {
      "corsRuleName": "production-web-access",
      "allowedOrigins": ["https://projects.everylanguage.com"],
      "allowedHeaders": [
        "authorization",
        "content-type",
        "content-length",
        "x-bz-file-name",
        "x-bz-content-sha1"
      ],
      "allowedOperations": [
        "b2_upload_file",
        "b2_download_file_by_id",
        "b2_download_file_by_name"
      ],
      "exposeHeaders": [
        "x-bz-content-sha1",
        "x-bz-file-name",
        "x-bz-file-id",
        "x-bz-upload-timestamp",
        "content-length",
        "content-type",
        "content-range",
        "content-disposition"
      ],
      "maxAgeSeconds": 3600
    }
  ]
}

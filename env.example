# Local Development Environment Variables
# Copy this file to .env.local and fill in your values

# Supabase Local Development (automatically set when running `supabase start`)
SUPABASE_URL=http://127.0.0.1:54321
SUPABASE_ANON_KEY=your_local_anon_key_from_supabase_start
SUPABASE_SERVICE_ROLE_KEY=your_local_service_role_key_from_supabase_start

# Local Database (automatically available with Supabase)
DATABASE_URL=postgresql://postgres:postgres@127.0.0.1:54322/postgres

# External Services (for local testing - use test credentials)
BACKBLAZE_B2_APPLICATION_KEY_ID=your_test_key_id
BACKBLAZE_B2_APPLICATION_KEY=your_test_key
BACKBLAZE_B2_BUCKET_NAME=your_test_bucket
BACKBLAZE_B2_REGION=us-west-004

# AI Services (for local development)
OPENAI_API_KEY=your_openai_test_key

# Feature Flags (for local development)
DISABLE_EXTERNAL_APIS=false
DEBUG_MODE=true
LOG_LEVEL=debug

# Note: Production environment variables are managed through GitHub Secrets
# and Supabase project settings. Never commit real API keys to version control! 